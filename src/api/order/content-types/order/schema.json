{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "orderNumber": {
      "type": "uid"
    },
    "email": {
      "type": "email"
    },
    "orderStatus": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "paid",
        "failed",
        "cancelled",
        "delivered"
      ]
    },
    "totalAmount": {
      "type": "decimal"
    },
    "currency": {
      "type": "enumeration",
      "enum": [
        "eur",
        "usd"
      ]
    },
    "products": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::product.product",
      "inversedBy": "orders"
    },
    "paymentId": {
      "type": "string"
    },
    "deliveryStatus": {
      "type": "enumeration",
      "enum": [
        "not_delivered",
        "delivered"
      ]
    },
    "notes": {
      "type": "text"
    },
    "customerName": {
      "type": "string"
    },
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "orders"
    }
  }
}
